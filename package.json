{
  "name": "onkel-ragekniv",
  "version": "1.0.0",
  "description": "AI-powered journalism platform for Apropos Magazine - article ingestion, AI article generation, and Webflow CMS integration",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests configured' && exit 0",
    "data:export": "node scripts/export-data.mjs",
    "build:hosted": "npm run data:export && next build",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "perf": "npm run build && npm run lighthouse",
    "bundle:analyze": "ANALYZE=true npm run build",
    "bundle:size": "npm run build && npx bundlesize",
    "ingest:rage": "npx tsx src/cli/ingest-rage.ts",
    "scrape:apropos": "npx tsx scripts/scrape-apropos-articles.ts",
    "optimize:templates": "npx tsx scripts/optimize-templates.ts",
    "train:tov": "npx tsx scripts/train-apropos-tov.ts"
  },
  "keywords": ["journalism", "ai", "webflow", "cms", "content-generation", "article-ingestion"],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@splinetool/runtime": "^1.10.76",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.90.5",
    "@types/cheerio": "^0.22.35",
    "@types/node": "24.3.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^5.3.0",
    "firebase": "^12.3.0",
    "jsonlines": "0.1.1",
    "lucide-react": "0.542.0",
    "next": "15.5.2",
    "openai": "^6.1.0",
    "pino": "^10.0.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "sharp": "^0.34.4",
    "web-vitals": "^5.1.0",
    "webflow-api": "^3.2.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@next/bundle-analyzer": "^15.5.6",
    "@next/eslint-plugin-next": "^16.0.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "19.1.12",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.13",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}
